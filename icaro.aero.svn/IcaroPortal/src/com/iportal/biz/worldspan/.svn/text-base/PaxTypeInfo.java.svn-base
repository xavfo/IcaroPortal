		this.clear();
	}
	
	public void clear () {
		this.paxTypeCode = null;
		this.baseFare = null;
		this.totalFare = null;
		this.numberOfPax = null;
		this.etickectReserved = null;
		this.reserved = null;
		this.baseFareCurrencyCode = null;
		this.totalFareCurrencyCode = null;
		this.paxType = null;
		
		this.ivaRate = null;
		this.tax1 = null;
		this.tax2 = null;
		this.tax3 = null;

	}
	
	

	public PaxType getPaxType() {
		return paxType;
	}

	public void setPaxType(PaxType paxType) {
		this.paxType = paxType;
	}

	public Double getBaseFare() {
		return baseFare;
	}

	public String getBaseFareCurrencyCode() {
		return baseFareCurrencyCode;
	}

	public String getEtickectReserved() {
		return etickectReserved;
	}

	public Integer getNumberOfPax() {
		return numberOfPax;
	}

	public String getPaxTypeCode() {
		return paxTypeCode;
	}

	public String getReserved() {
		return reserved;
	}

	public Double getTotalFare() {
		return totalFare;
	}

	public String getTotalFareCurrencyCode() {
		return totalFareCurrencyCode;
	}

	public void setBaseFare(Double baseFare) {
		this.baseFare = baseFare;
	}

	public void setBaseFareCurrencyCode(String baseFareCurrencyCode) {
		this.baseFareCurrencyCode = baseFareCurrencyCode;
	}

	public void setEtickectReserved(String etickectReserved) {
		this.etickectReserved = etickectReserved;
	}

	public void setNumberOfPax(Integer numberOfPax) {
		this.numberOfPax = numberOfPax;
	}

	public void setPaxTypeCode(String paxTypeCode) {
		this.paxTypeCode = paxTypeCode;
	}

	public void setReserved(String reserved) {
		this.reserved = reserved;
	}

	public void setTotalFare(Double totalFare) {
		this.totalFare = totalFare;
	}

	public void setTotalFareCurrencyCode(String totalFareCurrencyCode) {
		this.totalFareCurrencyCode = totalFareCurrencyCode;
