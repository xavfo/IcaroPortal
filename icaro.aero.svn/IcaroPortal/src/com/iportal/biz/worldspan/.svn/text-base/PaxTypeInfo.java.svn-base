package com.iportal.biz.worldspan;

import com.iportal.model.icaro.fare.PaxType;

/**
 * @author hernan
 *
 */
public class PaxTypeInfo {

	private String paxTypeCode;
	private Integer numberOfPax;
	
	private Double baseFare;
	private Double totalFare;
	
	private String baseFareCurrencyCode;//ISO Currency Code of base fare amount
	
	private String totalFareCurrencyCode;//ISO Currency Code of base fare amount
	
	
	private String etickectReserved;
	private String reserved;
	
	//DATOS PARA PRESENTAR EN PANTALLA
	
