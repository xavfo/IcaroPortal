package com.iportal.ctrl.service;

import javax.servlet.http.HttpServletRequest;

import org.apache.struts.action.ActionMapping;

import com.iportal.ctrl.BasePortalForm;

/**
 * comment ContactForm
 *
 * @author  YAGE (monica)
 * @version 1.0
 *
 */
public class EmailForm extends BasePortalForm {

    private static final long serialVersionUID = -9175419142650847678L;

    private String firstName;

    private String lastName;

    private String email;

    private String institution;

    private String address;

    private String phone;

    private String cityName;

    private String countryName;

    private Long countryCode;

    private String subject;

    private String comments;

    /**
     * Creates a new instance of BasePortalForm
     */
    public EmailForm() {
        super();
    }


    public void reset(ActionMapping mapping, HttpServletRequest request) {
        super.reset(mapping, request);
        firstName= null;
        lastName = null;
        email = null;
        address = null;
        phone = null;
        cityName = null;
        countryName = null;
        countryCode = null;
        subject = null;
        comments = null;
        institution = null;
    }

    /**
     * @return Returns the address.
     */
    public String getAddress() {
        return address;
    }
    /**
     * @return Returns the cityName.
     */
    public String getCityName() {
        return cityName;
    }
    /**
     * @return Returns the comments.
     */
    public String getComments() {
        return comments;
    }
    /**
     * @return Returns the countryCode.
     */
    public Long getCountryCode() {
        return countryCode;
    }
    /**
     * @return Returns the countryName.
     */
    public String getCountryName() {
        return countryName;
    }
    /**
     * @return Returns the email.
     */
    public String getEmail() {
        return email;
    }
    /**
     * @return Returns the firstName.
     */
    public String getFirstName() {
        return firstName;
    }
    /**
     * @return Returns the institution.
     */
    public String getInstitution() {
        return institution;
    }
    /**
     * @return Returns the lastName.
     */
    public String getLastName() {
        return lastName;
    }
    /**
     * @return Returns the phone.
     */
    public String getPhone() {
        return phone;
    }
    /**
     * @return Returns the subject.
     */
    public String getSubject() {
        return subject;
    }
    /**
     * @param address The address to set.
     */
    public void setAddress(String address) {
        this.address = address;
    }
    /**
     * @param cityName The cityName to set.
     */
    public void setCityName(String cityName) {
        this.cityName = cityName;
    }
    /**
     * @param comments The comments to set.
     */
    public void setComments(String comments) {
        this.comments = comments;
    }
    /**
     * @param countryCode The countryCode to set.
     */
    public void setCountryCode(Long countryCode) {
        this.countryCode = countryCode;
    }
    /**
     * @param countryName The countryName to set.
     */
    public void setCountryName(String countryName) {
        this.countryName = countryName;
    }
    /**
     * @param email The email to set.
     */
    public void setEmail(String email) {
        this.email = email;
    }
    /**
     * @param firstName The firstName to set.
     */
    public void setFirstName(String firstName) {
        this.firstName = firstName;
    }
    /**
     * @param institution The institution to set.
     */
    public void setInstitution(String institution) {
        this.institution = institution;
    }
    /**
     * @param lastName The lastName to set.
     */
    public void setLastName(String lastName) {
        this.lastName = lastName;
    }
    /**
     * @param phone The phone to set.
     */
    public void setPhone(String phone) {
        this.phone = phone;
    }
    /**
     * @param subject The subject to set.
     */
    public void setSubject(String subject) {
        this.subject = subject;
    }
}
