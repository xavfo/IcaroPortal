package com.yage.commons;

/**
 * @author hernan
 *
 */
public class IntegerBean {
	
	private Integer value;
	
    public IntegerBean() {
        super();
        this.value = null;
    }

    public IntegerBean(Integer value) {
        super();
        this.value = value;
    }

	
    @Override
    public String toString() {
        return this.value!= null?this.value.toString():super.toString();
    }

	public Integer getValue() {
		return value;
	}

	public void setValue(Integer value) {
		this.value = value;
	}

}
